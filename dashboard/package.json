{
  "name": "Chromie-dashboard",
  "version": "1.0.0",
  "description": "Advanced Discord bot dashboard with OAuth authentication",
  "main": "dist/server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "tsx watch src/server/server.ts",
    "client:dev": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "vite build",
    "start": "node dist/server/server.js",
    "migrate": "node -r ts-node/register migrations/migrate.ts"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "knex": "^3.1.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/csurf": "^1.11.5",
    "@types/express": "^4.17.25",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.19.25",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.15",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  }
}
